{
    "pipeline": [
        {
            "type": "readers.las",
            "filename": "input.laz"
        },
        {
            "type": "filters.range",
            "limits": "Z[1100:2800]"
        },
        {
            "type": "filters.elm"
        },
        {
            "type": "filters.outlier",
            "method": "statistical",
            "mean_k": 160,
            "multiplier": 2.5
        },
        {
            "type": "filters.csf",
            "cloth_resolution": 0.75,
            "rigidness": 1,
            "iterations": 800,
            "threshold": 0.2,
            "classification": true
        },
        {
            "type": "filters.hag_delaunay"
        },
        {
            "type": "filters.range",
            "limits": "HeightAboveGround[0:35]"
        },
        {
            "type": "filters.expression",
            "expression": "((Classification >= 2) && (Classification <= 5))"
        },
        {
            "type": "writers.las",
            "filename": output_file,
            "compression": "false",
            "minor_version": "4",
            "forward": "all",
        },
    ]
}