{
  "processor_settings": {
    "group": 22,
    "sampling_strategy": "adjacent",
    "keep_variables": [
      "X",
      "Y",
      "Z",
      "Intensity",
      "Classification",
      "HeightAboveGround"
    ],
    "n_jobs": 11,
    "crop": 1
  },
  "pdal_pipeline": {
    "phase_1": [
      {
        "type": "filters.outlier",
        "method": "statistical",
        "mean_k": 20,
        "multiplier": 3.0
      },
      {
        "type": "filters.assign",
        "value": [
          "NumberOfReturns = 1",
          "ReturnNumber = 1"
        ]
      },
      {
        "type": "filters.smrf",
        "slope": 0.75,
        "window": 25,
        "threshold": 0.3,
        "scalar": 2.0,
        "cell": 2.85,
        "ignore": "Classification[7:7]"
      },
      {
        "type": "filters.elm",
        "cell": 3
      }
    ],
    "phase_2": [
      {
        "type": "filters.hag_delaunay",
        "count": 80,
        "allow_extrapolation": true
      },
      {
        "type": "filters.range",
        "limits": "HeightAboveGround[0:35]"
      },
      {
        "type": "filters.merge"
      }
    ]
  },
  "paths": {
    "input_path": "/mnt/crea_camtrap/LIDAR/RAW/",
    "output_dir": "/mnt/sentinel4To/LIDAR_RUNEXP_MB",
    "lidar_list_tiles": "/mnt/crea_camtrap/LIDAR/SHP/TA_programme_lidar_HD_path.shp",
    "area_of_interest": "/mnt/crea_camtrap/LIDAR/SHP/Emprise_totale_3massifs_EPSG2154_V2.shp"
  }
}