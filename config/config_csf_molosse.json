{
  "processor_settings": {
    "group": 20,
    "sampling_strategy": "random",
    "keep_variables": [
      "X",
      "Y",
      "Z",
      "Intensity",
      "Classification",
      "HeightAboveGround"
    ],
    "n_jobs": 10,
    "crop": 1
  },
  "pdal_pipeline": {
    "phase_1": [
      {
        "type": "filters.outlier",
        "method": "statistical",
        "mean_k": 40,
        "multiplier": 1.5
      },
      {
        "type": "filters.smrf",
        "slope": 0.275,
        "window": 10,
        "threshold": 0.3,
        "scalar": 1.9,
        "cell": 1,
        "ignore": "Classification[7:7]"
      },
      {
        "type": "filters.elm",
        "cell": 3
      },
      {
        "type": "filters.hag_delaunay",
        "count": 40
      },
      {
        "type": "filters.range",
        "limits": "HeightAboveGround[0:35]"
      }
    ],
    "phase_2": [
      {
        "type": "filters.merge"
      }
    ]
  },
  "paths": {
    "input_path": "/mnt/crea_camtrap/LIDAR/RAW/MontBlanc/peclerey",
    "output_dir": "/mnt/sentinel4To/LIDAR_RUNEXP_MB",
    "lidar_list_tiles": "/mnt/crea_camtrap/LIDAR/SHP/TA_programme_lidar_HD_path.shp",
    "area_of_interest": "/mnt/crea_camtrap/LIDAR/SHP/Emprise_totale_3massifs_EPSG2154_V2.shp"
  }
}